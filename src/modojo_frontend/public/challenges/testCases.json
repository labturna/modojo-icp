                    "expected": "+3"
                },
                {
                    "functionName": "lengthOfLongestSubstring",
                    "description": "Find the length of the longest substring without repeating characters in 'bbbbb'",
                    "input": "bbbbb",
                    "expected": "+1"
                },
                {
                    "functionName": "lengthOfLongestSubstring",
                    "description": "Find the length of the longest substring without repeating characters in 'pwwkew'",
                    "input": "pwwkew",
                    "expected": "+3"
                }
            ]
        },
        {
            "slug": "threeSum",
            "name": "3Sum",
            "difficulty": "medium",
            "tests": [{
                    "functionName": "threeSum",
                    "description": "Find all unique triplets in the array which gives the sum of zero",
                    "input": [-1, 0, 1, 2, -1, -4],
                    "expected": "[[-1, -1, 2], [-1, 0, 1]]"
                },
                {
                    "functionName": "threeSum",
                    "description": "Find all unique triplets in the array which gives the sum of zero",
                    "input": [0, 0, 0],
                    "expected": "[[0, 0, 0]]"
                },
                {
                    "functionName": "threeSum",
                    "description": "Find all unique triplets in the array which gives the sum of zero",
                    "input": [0, 0, 0, 0],
                    "expected": "[[0, 0, 0]]"
                }
            ]
        },
        {
            "slug": "threeSumClosest",
