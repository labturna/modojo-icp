                    },
                    "expected": "+2"
                },
                {
                    "functionName": "threeSumClosest",
                    "description": "Find the sum of the three integers that is closest to the target",
                    "input": {
                        "nums": [1, 1, 1, 0],
                        "target": 100
                    },
                    "expected": "+3"
                }
            ]
        },
